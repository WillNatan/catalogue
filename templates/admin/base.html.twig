<script>
  function logout(href) {
    swal.fire({
      title: 'Déconnexion',
      text: 'Voulez-vous vraiment vous déconnecter?',
      icon: 'error',
      confirmButtonColor: '#ff6600',
      cancelButtonColor: '#f0f0f0',
      confirmButtonText: 'Oui',
      showCancelButton: true,
      cancelButtonText: "Non"
    }).then((result) => {
      if (result.value) {
        window.location.href = href;
      }
    });
  }
    $('#logoutbtn').click(function (e) {
      e.preventDefault();
      logout($(this).attr('href'))
    });


    $('#deleteItem').click(function (e) {
      e.preventDefault();
      swal.fire({
        title: 'Attention !',
        text: 'Voulez-vous vraiment effectuer cette action ?',
        icon: 'error',
        confirmButtonColor: '#f0f0f0',
        cancelButtonColor: '#ff6600',
        confirmButtonText: 'Oui, supprimer',
        showCancelButton: true,
        cancelButtonText: "Non"
      }).then(
              result =>{
                if(result.value){
                  swal.fire({
                    type: 'Success',
                    html: 'Vous avez supprimé la ligne.'
                  });
                  $('#deleteItem').submit();
                }else{
                  return false;
                }
              }
              )
    })
</script>
