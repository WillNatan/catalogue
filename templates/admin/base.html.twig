<div class="modal fade " id="secretResponseModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content card">
      <div class="modal-header card-header-warning">
        <h5 class="modal-title" id="exampleModalLabel">Définissez une question et une réponse secrète</h5>
      </div>
      <form action="{{ path('Administration') }}" method="post">
      <div class="modal-body">
        <blockquote class="blockquote text-center">
          <p class="text-primary">Elle vous servira lors de la demande de réinitialisation de mot de passe.</p>
        </blockquote>
          <div class="form-group">
            <select name="questionSecrete" class="form-control" required>
              <option value="" selected disabled>Sélectionnez une question secrète</option>
              <option value="Le nom de votre animal de compagnie">Le nom de votre animal de compagnie</option>
              <option value="Le modèle de votre premier véhicule">Le modèle de votre premier véhicule</option>
              <option value="Le métier de votre grand père">Le métier de votre grand père</option>
              <option value="Le nom de votre boisson préférée">Le nom de votre boisson préférée</option>
              <option value="Le nom de votre dessert préféré">Le nom de votre dessert préféré</option>
              <option value="Où êtes-vous allé la première fois que vous avez pris l'avion ?">Où êtes-vous allé la première fois que vous avez pris l'avion ?</option>
            </select>
          </div>
        <div class="form-group">
          <input type="text" class="form-control" required name="reponseSecrete" placeholder="Votre réponse secrète">
        </div>
      </div>
      <div class="modal-footer">
        <button type="submit" class="btn btn-warning">Valider</button>
      </div>
      </form>
    </div>
  </div>
</div>

<!--   Core JS Files   -->
