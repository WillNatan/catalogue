<script>
  {% if app.user.responseSecrete is null %}
  $('#secretResponseModal').modal(
          {backdrop: 'static', keyboard: false}
  );
  {% endif %}

  $('#search-btn').click(function () {
    $('#searchModal').modal(
    );
  });
  $('#search-input').keyup(function () {
    $.post('{{ path('recherche-rapport') }}',
            {
              t : $('#search-input').val()
            },
            function (data) {
              $('.results').empty();
              if($('#search-input').val() === "")
              {
                $('.results').empty();
              }
              else{
                data.map((report, index) =>(

                        $('.results').append(
                                '<div class="col">' +
                                '<div class="card">' +
                                ' <div class="card-body text-center">' +
                                '<div class="row">' +
                                '<div class="col-12">' +
                                '<i class="material-icons">description</i>' +
                                '</div>' +
                                '<div class="col-12">' +
                                '<a href="{{ path('Catalogue-des-rapports') }}'+report.id+'">'+report.Nom_Rapport+'</a>' +
                                '</div>' +
                                '</div>' +
                                ' </div>' +
                                '</div>' +
                                '</div>'
                        )
                ))
              }
            }
    )
  })
</script>
