<script src="{{ asset('back/js/material-dashboard.min40a0.js') }}"></script>
<script src="{{ asset('back/js/admin-custom.js') }}"></script>
<script>
  {% if app.user.responseSecrete is null %}
  $('#secretResponseModal').modal(
          {backdrop: 'static', keyboard: false}
  );
  {% endif %}
</script>

